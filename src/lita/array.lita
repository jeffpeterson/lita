class Array
	fn << (x) = this.push(x)

	fn == (b)
		if length != b.length: return false
		if object_id == b.object_id: return true
		let i = 0
		while i < length
			if get(i) != b.get(i++): return false
		return true

	fn each(f)
		let i = 0
		while i < length
			f(get(i++))

	fn map(f)
		let new = []
		let i = 0
		while i < length
			new << f(get(i++))
		return new

	fn print(io)
		each(.print(io))
