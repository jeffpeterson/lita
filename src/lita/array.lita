class Array
	fn << (x) = this.push(x)

	fn == (b)
		if length != b.length: return false
		if object_id == b.object_id: return true
		for let i = 0; i < length; i++
			if get(i) != b.get(i): return false
		return true

	fn map(f)
		let new = []
		for let i = 0; i < length; i++
			new << f(get(i))
		return new
